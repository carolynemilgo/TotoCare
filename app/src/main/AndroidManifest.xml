<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.group.totocare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".User_Interface.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Fragments.HomeFragment" />
        <activity android:name=".Fragments.BabyFragment" />
        <activity android:name=".Fragments.CheckUpsFragment" />
        <activity android:name=".Fragments.CommunityFragment" />
        <activity android:name=".User_Interface.SettingsActivity"></activity>
    </application>

</manifest>